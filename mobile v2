-- Scripts:

local function AWHU_fake_script() -- KeySystemFrame.drag 
	local script = Instance.new('LocalScript', KeySystemFrame)

	
	
	local frame = script.Parent
	local userInputService = game:GetService("UserInputService")
	local runService = game:GetService("RunService")
	
	local dragging = false
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		frame.Position = UDim2.new(
			startPos.X.Scale,
			startPos.X.Offset + delta.X,
			startPos.Y.Scale,
			startPos.Y.Offset + delta.Y
		)
	end
	
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1
			or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = frame.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement
			or input.UserInputType == Enum.UserInputType.Touch then
			if dragging then
				update(input)
			end
		end
	end)
	
end
coroutine.wrap(AWHU_fake_script)()
local function JHPYRKI_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	
	
	-- CONFIGURATION
	local CORRECT_KEY = "Yb7%Tg2^hK9!sLm4" -- change this to your own key
	
	-- SERVICES
	local TweenService = game:GetService("TweenService")
	
	-- REFERENCES
	local textButton = script.Parent
	local keySystemFrame = textButton.Parent
	local textBox = keySystemFrame:WaitForChild("TextBox")
	local sharedKey = textBox:WaitForChild("EnteredKey")
	
	local mainFrame = keySystemFrame.Parent
	local screenGui = mainFrame.Parent
	local welcomeFrame = screenGui:WaitForChild("WelcomeFrame")
	
	-- INITIAL SETUP
	welcomeFrame.Visible = false
	welcomeFrame.BackgroundTransparency = 0.2
	
	-- TWEEN SETTINGS
	local fadeTweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	
	-- FUNCTION: Vibrate animation (short, subtle shake)
	local function vibrateButton(button)
		local originalPos = button.Position
		for i = 1, 6 do
			local offsetX = math.random(-4, 4)
			local offsetY = math.random(-2, 2)
			button.Position = originalPos + UDim2.new(0, offsetX, 0, offsetY)
			task.wait(0.02)
		end
		button.Position = originalPos
	end
	
	-- FUNCTION: Fade in WelcomeFrame (respects your custom transparency)
	local function fadeInWelcomeFrame()
		welcomeFrame.Visible = true
		local fadeIn = TweenService:Create(welcomeFrame, fadeTweenInfo, {BackgroundTransparency = 0.2})
		fadeIn:Play()
	end
	
	-- MAIN FUNCTION
	textButton.MouseButton1Click:Connect(function()
		local enteredKey = sharedKey.Value
	
		if enteredKey == CORRECT_KEY then
			-- ‚úÖ CORRECT KEY: Fade transition
			local fadeOut = TweenService:Create(mainFrame, fadeTweenInfo, {BackgroundTransparency = 1})
			fadeOut:Play()
			task.wait(1)
			mainFrame.Visible = false
			fadeInWelcomeFrame()
	
		else
			-- ‚ùå INCORRECT KEY: Red text + vibrate + reset to white text
			textBox.Text = "Incorrect, please try again."
			textBox.TextColor3 = Color3.fromRGB(255, 0, 0)
			vibrateButton(textButton)
	
			task.wait(1.5)
	
			-- Reset text color to white
			textBox.Text = ""
			textBox.TextColor3 = Color3.fromRGB(255, 255, 255)
		end
	end)
	
end
coroutine.wrap(JHPYRKI_fake_script)()
local function EMUJOB_fake_script() -- TextBox.LocalScript 
	local script = Instance.new('LocalScript', TextBox)

	
	
	local textBox = script.Parent
	
	-- Create a shared variable for the entered key
	local sharedKey = Instance.new("StringValue")
	sharedKey.Name = "EnteredKey"
	sharedKey.Value = ""
	sharedKey.Parent = textBox
	
	-- Update whenever the player types
	textBox:GetPropertyChangedSignal("Text"):Connect(function()
		sharedKey.Value = textBox.Text
	end)
	
end
coroutine.wrap(EMUJOB_fake_script)()
local function HBME_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	
	
	local textButton = script.Parent
	local textToCopy = "https://discord.gg/ThyCc86tjp" -- The text that will be copied
	
	textButton.MouseButton1Click:Connect(function()
		local success, err = pcall(function()
			-- Prompt player to copy the string
			game:GetService("StarterGui"):SetCore("PromptCopyString", textToCopy)
		end)
	
		if success then
			-- Show notification popup
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "SYSTEM SAYS:",
				Text = "Succesfully Copied '" .. textToCopy .. "'the link was copied into your clipboard.",
				Duration = 3
			})
		else
			-- Show error if something goes wrong
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Error",
				Text = "Could not copy text: " .. tostring(err),
				Duration = 3
			})
		end
	end)
	
end
coroutine.wrap(HBME_fake_script)()
local function DBSDJN_fake_script() -- ImageButton.LocalScript 
	local script = Instance.new('LocalScript', ImageButton)

	
	
	local TweenService = game:GetService("TweenService")
	
	-- UI references
	local imageButton = script.Parent
	local screenGui = imageButton.Parent
	local mainFrame = screenGui:WaitForChild("ScriptFrame")
	
	-- Sounds
	local hoverSound = imageButton:FindFirstChild("HoverSound")
	local clickSound = imageButton:FindFirstChild("ClickSound")
	
	-- Initial states
	mainFrame.Visible = false
	mainFrame.BackgroundTransparency = 1
	
	-- Tween settings
	local tweenInfo = TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	local hoverTweenInfo = TweenInfo.new(0.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	
	-- // Hover effect
	imageButton.MouseEnter:Connect(function()
		-- Play hover sound
		if hoverSound then hoverSound:Play() end
	
		-- Slightly shrink and dim the image
		local shrinkTween = TweenService:Create(imageButton, hoverTweenInfo, {
			Size = imageButton.Size - UDim2.new(0, 4, 0, 4),
			ImageColor3 = Color3.fromRGB(200, 200, 200)
		})
		shrinkTween:Play()
	end)
	
	-- // Mouse leaves (reset hover)
	imageButton.MouseLeave:Connect(function()
		local resetTween = TweenService:Create(imageButton, hoverTweenInfo, {
			Size = imageButton.Size + UDim2.new(0, 4, 0, 4),
			ImageColor3 = Color3.fromRGB(255, 255, 255)
		})
		resetTween:Play()
	end)
	
	-- // Click effect (fade in frame + click sound)
	imageButton.MouseButton1Click:Connect(function()
		-- Play click sound
		if clickSound then clickSound:Play() end
	
		-- Hide the button
		imageButton.Visible = false
	
		-- Make frame visible so it can fade in
		mainFrame.Visible = true
	
		-- Fade in the frame
		local fadeInTween = TweenService:Create(mainFrame, tweenInfo, {BackgroundTransparency = 0})
		fadeInTween:Play()
	
		-- Fade in all UI elements inside
		for _, object in pairs(mainFrame:GetDescendants()) do
			if object:IsA("TextLabel") or object:IsA("TextButton") or object:IsA("ImageLabel") or object:IsA("ImageButton") then
				object.BackgroundTransparency = 1
				if object:IsA("TextLabel") or object:IsA("TextButton") then
					object.TextTransparency = 1
				end
	
				local props = {BackgroundTransparency = 0}
				if object:IsA("TextLabel") or object:IsA("TextButton") then
					props.TextTransparency = 0
				end
	
				local tween = TweenService:Create(object, tweenInfo, props)
				tween:Play()
			end
		end
	end)
	
end
coroutine.wrap(DBSDJN_fake_script)()
local function BFBLNU_fake_script() -- ScriptFrame.drag 
	local script = Instance.new('LocalScript', ScriptFrame)

	
	
	local frame = script.Parent
	local userInputService = game:GetService("UserInputService")
	local runService = game:GetService("RunService")
	
	local dragging = false
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		frame.Position = UDim2.new(
			startPos.X.Scale,
			startPos.X.Offset + delta.X,
			startPos.Y.Scale,
			startPos.Y.Offset + delta.Y
		)
	end
	
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1
			or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = frame.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement
			or input.UserInputType == Enum.UserInputType.Touch then
			if dragging then
				update(input)
			end
		end
	end)
	
end
coroutine.wrap(BFBLNU_fake_script)()
local function AXNZ_fake_script() -- InfoButton.LocalScript 
	local script = Instance.new('LocalScript', InfoButton)

	
	local button = script.Parent
	local frame = script.Parent.Parent:WaitForChild("InformationFrame") -- change "Frame" if named differently
	
	-- optional: start hidden
	frame.Visible = false
	frame.BackgroundTransparency = 1
	
	-- tween service for smooth fade
	local TweenService = game:GetService("TweenService")
	
	-- settings
	local fadeTime = 0.3 -- seconds for fade animation
	
	-- function to fade in
	local function fadeIn()
		frame.Visible = true
		local tween = TweenService:Create(frame, TweenInfo.new(fadeTime), {BackgroundTransparency = 0})
		tween:Play()
	end
	
	-- function to fade out
	local function fadeOut()
		local tween = TweenService:Create(frame, TweenInfo.new(fadeTime), {BackgroundTransparency = 1})
		tween:Play()
		tween.Completed:Connect(function()
			frame.Visible = false
		end)
	end
	
	-- toggle on button click
	local isVisible = false
	button.MouseButton1Click:Connect(function()
		isVisible = not isVisible
		if isVisible then
			fadeIn()
		else
			fadeOut()
		end
	end)
	
end
coroutine.wrap(AXNZ_fake_script)()
