local function DGRULM_fake_script() -- AutoResetButton.LocalScript 
	local script = Instance.new('LocalScript', AutoResetButton)

	
	
	local button = script.Parent
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	
	local autoResetEnabled = false
	local connection
	
	local function toggleAutoReset()
		autoResetEnabled = not autoResetEnabled
	
		if autoResetEnabled then
			button.Text = "AutoReset: ON"
	
			local function setupHumanoid(humanoid)
				connection = humanoid.HealthChanged:Connect(function(health)
					if autoResetEnabled and health < humanoid.MaxHealth then
	
						humanoid.Health = 0
					end
				end)
			end
	
	
			if player.Character then
				local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
				if humanoid then
					setupHumanoid(humanoid)
				end
			end
	
	
			player.CharacterAdded:Connect(function(char)
				local humanoid = char:WaitForChild("Humanoid")
				setupHumanoid(humanoid)
			end)
		else
			button.Text = "AutoReset: OFF"
	
	
			if connection then
				connection:Disconnect()
				connection = nil
			end
		end
	end
	
	button.MouseButton1Click:Connect(toggleAutoReset)
	
	
	button.Text = "AutoReset: OFF"
	
end
coroutine.wrap(DGRULM_fake_script)()
local function KSQXGPG_fake_script() -- SetSpawnButton.LocalScript 
	local script = Instance.new('LocalScript', SetSpawnButton)

	local button = script.Parent
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	
	local stuckEnabled = false
	local stuckPosition = nil
	local loopConnection = nil
	
	local function startStuckLoop()
		if loopConnection then
			loopConnection:Disconnect()
		end
	
		loopConnection = game:GetService("RunService").Heartbeat:Connect(function()
			if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				player.Character:MoveTo(stuckPosition)
			end
		end)
	end
	
	
	local function onCharacterAdded(character)
		if stuckEnabled and stuckPosition then
	
			character:WaitForChild("HumanoidRootPart")
			startStuckLoop()
		end
	end
	
	local function toggleStuck()
		stuckEnabled = not stuckEnabled
	
		if stuckEnabled then
			button.Text = "SetSpawn"
	
	
			local char = player.Character
			if char and char:FindFirstChild("HumanoidRootPart") then
				stuckPosition = char.HumanoidRootPart.Position
				startStuckLoop()
			end
	
			player.CharacterAdded:Connect(onCharacterAdded)
		else
			button.Text = "SetSpawn"
			stuckPosition = nil
			if loopConnection then
				loopConnection:Disconnect()
				loopConnection = nil
			end
		end
	end
	
	button.MouseButton1Click:Connect(toggleStuck)
	
end
coroutine.wrap(KSQXGPG_fake_script)()
local function QPCYBQ_fake_script() -- OptimizationButton.LocalScript 
	local script = Instance.new('LocalScript', OptimizationButton)

	local button = script.Parent
	local player = game:GetService("Players").LocalPlayer
	local StarterGui = game:GetService("StarterGui")
	
	local clicked = false
	
	button.MouseButton1Click:Connect(function()
		if clicked then return end
		clicked = true
	
		local map = workspace:FindFirstChild("Map")
		if map then
			local trees = map:FindFirstChild("Trees")
			local walls = map:FindFirstChild("Walls")
			local grasstop = map:FindFirstChild("GrassTop")
	
			if grasstop then
				grasstop:Destroy()
			end
			if trees then
				trees:Destroy()
			end
			if walls then
				walls:Destroy()
			end
		end
	
	
		button.Text = "Optimization"
		button.Active = false
		button.AutoButtonColor = false
	
	
		StarterGui:SetCore("SendNotification", {
			Title = "SYSTEM SAYS:",
			Text = "Succesfully made your game smoother !", 
			Duration = 4, --
		})
	end)
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	--- PROPERTY OF INF YIELD .. 2/3..2.2.12/.2.1..
end
