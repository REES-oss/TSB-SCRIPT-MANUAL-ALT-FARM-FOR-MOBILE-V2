local function DXEU_fake_script() -- ScriptFrame.drag 
	local script = Instance.new('LocalScript', ScriptFrame)

	local UI = script.Parent
	
	local Dragging
	local Input
	local Start
	local StartPosition
	
	function Lerp(A, B, M)
		return A + (B - A) * M
	end
	
	local LMPos
	local LGPos
	
	local Speed = 13
	
	function Update(Time)
		if not StartPosition then
			return
		end
	
		if not Dragging and LGPos then
			UI.Position = UDim2.new(StartPosition.X.Scale, Lerp(UI.Position.X.Offset, LGPos.X.Offset, Time * Speed), StartPosition.Y.Scale, Lerp(UI.Position.Y.Offset, LGPos.Y.Offset, Time * Speed))
			return
		end
	
		local Delta = (LMPos - game:GetService("UserInputService"):GetMouseLocation())
	
		local xGoal = (StartPosition.X.Offset - Delta.X)
		local yGoal = (StartPosition.Y.Offset - Delta.Y)
	
		LGPos = UDim2.new(StartPosition.X.Scale, xGoal, StartPosition.Y.Scale, yGoal)
		UI.Position = UDim2.new(StartPosition.X.Scale, Lerp(UI.Position.X.Offset, xGoal, Time * Speed), StartPosition.Y.Scale, Lerp(UI.Position.Y.Offset, yGoal, Time * Speed))
	end
	
	UI.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			Dragging = true
			Start = input.Position
			StartPosition = UI.Position
			LMPos = game:GetService("UserInputService"):GetMouseLocation()
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					Dragging = false
				end
			end)
		end
	end)
	
	UI.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			Input = input
		end
	end)
	
	game:GetService("RunService").Heartbeat:Connect(Update)
end
coroutine.wrap(DXEU_fake_script)()
local function ZGSKJK_fake_script() -- InfoButton.LocalScript 
	local script = Instance.new('LocalScript', InfoButton)

	
	local button = script.Parent
	local frame = script.Parent.Parent:WaitForChild("InformationFrame") -- change "Frame" if named differently
	
	-- optional: start hidden
	frame.Visible = false
	frame.BackgroundTransparency = 1
	
	-- tween service for smooth fade
	local TweenService = game:GetService("TweenService")
	
	-- settings
	local fadeTime = 0.3 -- seconds for fade animation
	
	-- function to fade in
	local function fadeIn()
		frame.Visible = true
		local tween = TweenService:Create(frame, TweenInfo.new(fadeTime), {BackgroundTransparency = 0})
		tween:Play()
	end
	
	-- function to fade out
	local function fadeOut()
		local tween = TweenService:Create(frame, TweenInfo.new(fadeTime), {BackgroundTransparency = 1})
		tween:Play()
		tween.Completed:Connect(function()
			frame.Visible = false
		end)
	end
	
	-- toggle on button click
	local isVisible = false
	button.MouseButton1Click:Connect(function()
		isVisible = not isVisible
		if isVisible then
			fadeIn()
		else
			fadeOut()
		end
	end)
	
end
coroutine.wrap(ZGSKJK_fake_script)()
