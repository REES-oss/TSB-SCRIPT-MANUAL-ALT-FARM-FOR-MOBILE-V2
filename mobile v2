-- Scripts:

local function VEYR_fake_script() -- FZHUBFORPHONE.LocalScript 
	local script = Instance.new('LocalScript', FZHUBFORPHONE)

	
	local gui = script.Parent
	local tweenService = game:GetService("TweenService")
	
	local welcomeFrame = gui:WaitForChild("WelcomeFrame")
	local mainFrame = gui:WaitForChild("MainFrame")
	
	-- Ensure initial visibility
	welcomeFrame.Visible = true
	mainFrame.Visible = false
	
	-- Wait 3 seconds before switching
	task.wait(3)
	
	-- Optional: Fade out welcome frame
	local fadeOut = tweenService:Create(
		welcomeFrame,
		TweenInfo.new(0.6, Enum.EasingStyle.Sine, Enum.EasingDirection.Out),
		{BackgroundTransparency = 1}
	)
	fadeOut:Play()
	
	-- Fade out text/image elements inside WelcomeFrame
	for _, obj in ipairs(welcomeFrame:GetDescendants()) do
		if obj:IsA("TextLabel") or obj:IsA("TextButton") then
			tweenService:Create(obj, TweenInfo.new(0.6), {TextTransparency = 1}):Play()
		elseif obj:IsA("ImageLabel") then
			tweenService:Create(obj, TweenInfo.new(0.6), {ImageTransparency = 1}):Play()
		end
	end
	
	fadeOut.Completed:Wait()
	welcomeFrame.Visible = false
	
	-- Show and fade in MainFrame
	mainFrame.Visible = true
	mainFrame.BackgroundTransparency = 1
	
	for _, obj in ipairs(mainFrame:GetDescendants()) do
		if obj:IsA("TextLabel") or obj:IsA("TextButton") then
			obj.TextTransparency = 1
		elseif obj:IsA("ImageLabel") then
			obj.ImageTransparency = 1
		end
	end
	
	local fadeIn = tweenService:Create(
		mainFrame,
		TweenInfo.new(0.6, Enum.EasingStyle.Sine, Enum.EasingDirection.Out),
		{BackgroundTransparency = 0}
	)
	fadeIn:Play()
	
	for _, obj in ipairs(mainFrame:GetDescendants()) do
		if obj:IsA("TextLabel") or obj:IsA("TextButton") then
			tweenService:Create(obj, TweenInfo.new(0.6), {TextTransparency = 0}):Play()
		elseif obj:IsA("ImageLabel") then
			tweenService:Create(obj, TweenInfo.new(0.6), {ImageTransparency = 0}):Play()
		end
	end
	
end
coroutine.wrap(VEYR_fake_script)()
local function DJJAC_fake_script() -- KeySystemFrame.drag 
	local script = Instance.new('LocalScript', KeySystemFrame)

	local UI = script.Parent
	
	local Dragging
	local Input
	local Start
	local StartPosition
	
	function Lerp(A, B, M)
		return A + (B - A) * M
	end
	
	local LMPos
	local LGPos
	
	local Speed = 13
	
	function Update(Time)
		if not StartPosition then
			return
		end
	
		if not Dragging and LGPos then
			UI.Position = UDim2.new(StartPosition.X.Scale, Lerp(UI.Position.X.Offset, LGPos.X.Offset, Time * Speed), StartPosition.Y.Scale, Lerp(UI.Position.Y.Offset, LGPos.Y.Offset, Time * Speed))
			return
		end
	
		local Delta = (LMPos - game:GetService("UserInputService"):GetMouseLocation())
	
		local xGoal = (StartPosition.X.Offset - Delta.X)
		local yGoal = (StartPosition.Y.Offset - Delta.Y)
	
		LGPos = UDim2.new(StartPosition.X.Scale, xGoal, StartPosition.Y.Scale, yGoal)
		UI.Position = UDim2.new(StartPosition.X.Scale, Lerp(UI.Position.X.Offset, xGoal, Time * Speed), StartPosition.Y.Scale, Lerp(UI.Position.Y.Offset, yGoal, Time * Speed))
	end
	
	UI.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			Dragging = true
			Start = input.Position
			StartPosition = UI.Position
			LMPos = game:GetService("UserInputService"):GetMouseLocation()
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					Dragging = false
				end
			end)
		end
	end)
	
	UI.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			Input = input
		end
	end)
	
	game:GetService("RunService").Heartbeat:Connect(Update)
end
coroutine.wrap(DJJAC_fake_script)()
local function RIIZBUT_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	-- TextButton LocalScript
	
	-- CONFIGURATION
	local CORRECT_KEY = "Yb7%Tg2^hK9!sLm4" -- change this to your own key
	
	-- SERVICES
	local TweenService = game:GetService("TweenService")
	
	-- REFERENCES
	local textButton = script.Parent
	local keySystemFrame = textButton.Parent
	local textBox = keySystemFrame:WaitForChild("TextBox")
	local sharedKey = textBox:WaitForChild("EnteredKey")
	
	local mainFrame = keySystemFrame.Parent
	local screenGui = mainFrame.Parent
	local scriptFrame = screenGui:WaitForChild("ScriptFrame")
	
	-- INITIAL SETUP
	scriptFrame.Visible = false
	scriptFrame.BackgroundTransparency = 1
	
	-- TWEEN SETTINGS
	local fadeTweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	
	-- FUNCTION: Vibrate animation (short, subtle shake)
	local function vibrateButton(button)
		local originalPos = button.Position
		for i = 1, 6 do
			local offsetX = math.random(-4, 4)
			local offsetY = math.random(-2, 2)
			button.Position = originalPos + UDim2.new(0, offsetX, 0, offsetY)
			task.wait(0.02)
		end
		button.Position = originalPos
	end
	
	-- FUNCTION: Fade in ScriptFrame
	local function fadeInScriptFrame()
		scriptFrame.Visible = true
		local fadeIn = TweenService:Create(scriptFrame, fadeTweenInfo, {BackgroundTransparency = 0})
		fadeIn:Play()
	end
	
	-- MAIN FUNCTION
	textButton.MouseButton1Click:Connect(function()
		local enteredKey = sharedKey.Value
	
		if enteredKey == CORRECT_KEY then
			-- ‚úÖ CORRECT KEY: Fade transition
			local fadeOut = TweenService:Create(mainFrame, fadeTweenInfo, {BackgroundTransparency = 1})
			fadeOut:Play()
			task.wait(1)
			mainFrame.Visible = false
			fadeInScriptFrame()
	
		else
			-- ‚ùå INCORRECT KEY: Red text + vibrate + reset to white text
			textBox.Text = "Incorrect, please try again."
			textBox.TextColor3 = Color3.fromRGB(255, 0, 0)
			vibrateButton(textButton)
	
			task.wait(1.5)
	
			-- Reset text color to white
			textBox.Text = ""
			textBox.TextColor3 = Color3.fromRGB(255, 255, 255)
		end
	end)
	
end
coroutine.wrap(RIIZBUT_fake_script)()
local function WHAR_fake_script() -- TextBox.LocalScript 
	local script = Instance.new('LocalScript', TextBox)

	
	
	local textBox = script.Parent
	
	-- Create a shared variable for the entered key
	local sharedKey = Instance.new("StringValue")
	sharedKey.Name = "EnteredKey"
	sharedKey.Value = ""
	sharedKey.Parent = textBox
	
	-- Update whenever the player types
	textBox:GetPropertyChangedSignal("Text"):Connect(function()
		sharedKey.Value = textBox.Text
	end)
	
end
coroutine.wrap(WHAR_fake_script)()
local function TDQDNEQ_fake_script() -- ImageButton.LocalScript 
	local script = Instance.new('LocalScript', ImageButton)

	
	
	local TweenService = game:GetService("TweenService")
	
	-- UI references
	local imageButton = script.Parent
	local screenGui = imageButton.Parent
	local mainFrame = screenGui:WaitForChild("ScriptFrame")
	
	-- Sounds
	local hoverSound = imageButton:FindFirstChild("HoverSound")
	local clickSound = imageButton:FindFirstChild("ClickSound")
	
	-- Initial states
	mainFrame.Visible = false
	mainFrame.BackgroundTransparency = 1
	
	-- Tween settings
	local tweenInfo = TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	local hoverTweenInfo = TweenInfo.new(0.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	
	-- // Hover effect
	imageButton.MouseEnter:Connect(function()
		-- Play hover sound
		if hoverSound then hoverSound:Play() end
	
		-- Slightly shrink and dim the image
		local shrinkTween = TweenService:Create(imageButton, hoverTweenInfo, {
			Size = imageButton.Size - UDim2.new(0, 4, 0, 4),
			ImageColor3 = Color3.fromRGB(200, 200, 200)
		})
		shrinkTween:Play()
	end)
	
	-- // Mouse leaves (reset hover)
	imageButton.MouseLeave:Connect(function()
		local resetTween = TweenService:Create(imageButton, hoverTweenInfo, {
			Size = imageButton.Size + UDim2.new(0, 4, 0, 4),
			ImageColor3 = Color3.fromRGB(255, 255, 255)
		})
		resetTween:Play()
	end)
	
	-- // Click effect (fade in frame + click sound)
	imageButton.MouseButton1Click:Connect(function()
		-- Play click sound
		if clickSound then clickSound:Play() end
	
		-- Hide the button
		imageButton.Visible = false
	
		-- Make frame visible so it can fade in
		mainFrame.Visible = true
	
		-- Fade in the frame
		local fadeInTween = TweenService:Create(mainFrame, tweenInfo, {BackgroundTransparency = 0})
		fadeInTween:Play()
	
		-- Fade in all UI elements inside
		for _, object in pairs(mainFrame:GetDescendants()) do
			if object:IsA("TextLabel") or object:IsA("TextButton") or object:IsA("ImageLabel") or object:IsA("ImageButton") then
				object.BackgroundTransparency = 1
				if object:IsA("TextLabel") or object:IsA("TextButton") then
					object.TextTransparency = 1
				end
	
				local props = {BackgroundTransparency = 0}
				if object:IsA("TextLabel") or object:IsA("TextButton") then
					props.TextTransparency = 0
				end
	
				local tween = TweenService:Create(object, tweenInfo, props)
				tween:Play()
			end
		end
	end)
	
end
coroutine.wrap(TDQDNEQ_fake_script)()
