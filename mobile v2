coroutine.wrap(ZXJKJXN_fake_script)()
local function LQPMUZZ_fake_script() -- TextLabel_5.LocalScript 
	local script = Instance.new('LocalScript', TextLabel_5)

	local textLabel = script.Parent
	local TweenService = game:GetService("TweenService")
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	
	-- References
	local welcomeFrame = textLabel.Parent
	local screenGui = welcomeFrame.Parent
	local mainFrame = screenGui:WaitForChild("MainFrame")
	local scriptFrame = screenGui:WaitForChild("ScriptFrame")
	
	-- Initial setup
	scriptFrame.Visible = false
	scriptFrame.BackgroundTransparency = 1
	
	-- Set the welcome message
	textLabel.Text = "¬°Welcome, " .. player.Name .. ", to Fz Hub!"
	
	-- Function to fade out a frame
	local function fadeOutFrame(frame)
		local fadeTweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local tween = TweenService:Create(frame, fadeTweenInfo, {BackgroundTransparency = 1})
		tween:Play()
		tween.Completed:Wait()
		frame.Visible = false
	end
	
	-- Function to fade in a frame
	local function fadeInFrame(frame)
		local fadeTweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		frame.Visible = true
		local tween = TweenService:Create(frame, fadeTweenInfo, {BackgroundTransparency = 0})
		tween:Play()
	end
	
	-- MAIN LOGIC:
	-- Wait until MainFrame is hidden and WelcomeFrame becomes visible
	task.spawn(function()
		while mainFrame.Visible == true do
			task.wait(0.1)
		end
	
		if welcomeFrame.Visible == true then
			-- Wait 3 seconds while WelcomeFrame is visible
			task.wait(3)
	
			-- Then fade out WelcomeFrame and fade in ScriptFrame
			fadeOutFrame(welcomeFrame)
			fadeInFrame(scriptFrame)
		end
	end)
	
end
coroutine.wrap(LQPMUZZ_fake_script)()
